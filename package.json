{
  "name": "trello-list",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server",
    "dev": "concurrently --kill-others \"npm:dev:server\" \"npm:dev:codegen\"",
    "dev:server": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "dev:codegen": "graphql-codegen --watch --config codegen.yml",
    "build": "tsc -p . && cp src/schema.graphql dist/schema.graphql",
    "test": "jest -ci",
    "db:create": "sequelize db:create",
    "db:destroy": "dropdb --if-exists backend_development",
    "db:reset": "NODE_ENV=development npm run db:destroy && npm run db:create && npm run db:migrate",
    "db:migrate": "NODE_ENV=development sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:init": "sequelize init",
    "db:seed": "sequelize db:seed:all",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "big.js": "^6.1.1",
    "cookie-parser": "^1.4.5",
    "compression": "^1.7.4",
    "dotenv": "^8.6.0",
    "jest": "29.5.0",
    "jest-mock": "29.5.0",
    "express": "^4.17.1",
    "apollo-server-express": "^3.4.0",
    "got": "^11.8.2",
    "graphql": "^15.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "humps": "^2.0.1",
    "node-cache": "^5.1.2",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-resolvers": "1.20.0",
    "@types/big.js": "^6.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/humps": "^2.0.1",
    "@types/node": "^16.4.1",
    "concurrently": "^6.2.0",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
